# .env.example
# Copy to .env and configure for your needs

# === OSM Data Configuration ===
# URL for downloading OSM file
OSM_DOWNLOAD_URL="https://download.geofabrik.de/europe/germany-latest.osm.pbf"

# Region name (used for file naming)
OSM_REGION_NAME="germany"

# Path to PBF file (automatically set by script)
OSM_PBF_PATH="./data/raw/germany-latest.osm.pbf"

# === Processing Configuration ===
# Maximum recursion depth for relations
MAX_RELATION_DEPTH="10"

# Enable complex multipolygon processing
ENABLE_COMPLEX_MULTIPOLYGONS="true"

# === DuckDB Configuration ===
# Target environment for dbt
DBT_TARGET="dev"

# Memory and performance settings
DUCKDB_MEMORY_LIMIT="16GB"
DUCKDB_THREADS="4"

# Enable profiling
ENABLE_PROFILING="false"

# === Download Configuration ===
# Force file redownload
FORCE_REDOWNLOAD="false"

# Verify MD5 checksum (if available)
VERIFY_CHECKSUM="true"
